@import "/styles/color.wxss";@import "/styles/typography.wxss";@import "/styles/shadow.wxss";.button{border-radius:100rpx;color:var(--Foreground-Low);gap:10px;position:relative;transition:background-color .2s ease}.button,.button-label{align-items:center;display:-webkit-flex;display:flex;justify-content:center}.button-label{height:100%;width:100%}.button-loading{display:-webkit-flex;display:flex;height:48rpx;width:48rpx}.button-style-primary-default{background:var(--Foreground-Primary);border:2rpx solid var(--Foreground-Primary)}.button-style-primary-disabled{background:var(--Foreground-Disabled);border:2rpx solid var(--Foreground-Disabled)}.button-style-primary-loading{background:var(--Foreground-Primary);border:2rpx solid var(--Foreground-Primary)}.button-style-secondary-default{background:var(--Foreground-Medium)}.button-style-secondary-disabled,.button-style-secondary-loading{background:var(--Background-Low)}.button-style-tertiary-default{border:2rpx solid var(--Foreground-Primary);color:var(--Foreground-Primary)}.button-style-tertiary-disabled{border:2rpx solid var(--Foreground-Disabled);color:var(--Foreground-Disabled)}.button-style-tertiary-loading{border:2rpx solid var(--Foreground-Primary)}.button-style-tertiary-loading,.button-style-text-default{color:var(--Foreground-Primary)}.button-style-text-disabled{color:var(--Foreground-Disabled)}.button-style-text-loading{color:var(--Foreground-Primary)}.button-size-large{padding:28rpx 16rpx}.button-size-medium{padding:18rpx 16rpx}.button-size-small{flex-shrink:0;padding:10rpx 16rpx}.button-click{height:100%;position:absolute}