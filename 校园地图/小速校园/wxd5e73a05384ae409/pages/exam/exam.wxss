.page{background-color:#f8f8f8;height:100vh;padding-top:30rpx;width:100%}.exam-wrapper{background-color:#fff;border:.5px solid #fff;border-radius:10rpx;display:-webkit-flex;display:flex;margin:0 40rpx 20rpx;padding:20rpx}.exam-wrapper .left{margin-right:auto}.exam-wrapper .left .top{align-items:center;display:-webkit-flex;display:flex;justify-content:center;margin-bottom:10rpx}.exam-wrapper .left .top .tag{border-radius:30rpx;font-size:28rpx;font-weight:500;margin-right:10rpx;padding:5rpx 10rpx;white-space:nowrap}.exam-wrapper .left .top .coming{background-color:#f2f9f2;color:#148f77}.exam-wrapper .left .top .past{background-color:#f8ecec;color:#da6c77}.exam-wrapper .left .top .name{font-weight:700}.exam-wrapper .left .bottom{color:#999;font-size:32rpx}.exam-wrapper .right{white-space:nowrap}.exam-wrapper .right .top{margin-bottom:10rpx}.exam-wrapper .right .bottom,.exam-wrapper .right .top{color:#333;font-size:32rpx}