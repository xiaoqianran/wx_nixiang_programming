.btn{align-items:center;border:0rpx;border-radius:12rpx;box-sizing:border-box;display:-webkit-inline-flex;display:inline-flex;font-size:28rpx;height:64rpx;justify-content:center;line-height:1;margin-left:0;margin-right:0;overflow:visible;padding:0 30rpx;text-align:center;text-decoration:none;transform:translate(0rpx,0rpx)}.btn::after{display:none}.btn:not([class*="bg-"]){background-color:#f0f0f0}.btn[class*="border"]{background-color:initial}.btn[class*="border"]::after{border:1rpx solid;border-radius:12rpx;box-sizing:border-box;content:" ";display:block;height:200%;left:0;pointer-events:none;position:absolute;top:0;transform:scale(.5);transform-origin:0 0;width:200%;z-index:1}.btn[class*="bg-"]::after{display:none}.btn.small{font-size:20rpx;height:48rpx}.btn.mid,.btn.small{border-radius:10rpx;padding:0 20rpx}.btn.mid{font-size:24rpx;height:50rpx}.btn.large{border-radius:14rpx;font-size:32rpx;height:80rpx;padding:0 40rpx}.btn.btn-icon.small{height:48rpx;width:48rpx}.btn.btn-icon{border-radius:500rpx;height:64rpx;padding:0;width:64rpx}button.btn-icon.large{height:80rpx;width:80rpx}.btn.shadow-blur::before{filter:blur(6rpx);left:4rpx;opacity:.6;top:4rpx}.btn.button-hover{box-shadow:inset 0 0 8px rgba(0,0,0,.1);left:3rpx;opacity:.9;position:relative;top:3rpx;transform:scale(.95,.95)}.btn[disabled]{color:var(--white);opacity:.6}.btn.round{border-radius:5000rpx}.btn.round[class*="border"]::after{border-radius:1000rpx}.btn.border-bold::after{border:6rpx solid}