@import "/portal/style/public/article_list.wxss";@import "/portal/projects/TRIP1/style/skin.wxss";.main{display:-webkit-flex;display:flex;flex-direction:column;justify-content:flex-start;padding:0}.text-pic-list-box{padding:20rpx 0rpx 10rpx}.text-pic-list-box .item{padding:20rpx 30rpx}.text-pic-list-box .title-line{align-items:flex-start;display:-webkit-flex;display:flex;justify-content:space-between;width:100%}.text-pic-list-box .title-line .arrow{color:#666;text-align:right;width:100rpx}.text-pic-list-box .item .oprt{align-items:center;color:var(--projectColor);display:-webkit-flex;display:flex;font-size:27rpx;justify-content:center;line-height:1.5;margin:30rpx 0 10rpx}.text-pic-list-box .item .oprt .btn{padding:0 20rpx}.info-list-box{box-sizing:border-box;justify-content:center;padding:0rpx 0rpx 10rpx;width:100%}.info-list-box,.info-list-box .item{display:-webkit-flex;display:flex;flex-direction:column}.info-list-box .item{background-color:#fff;border-radius:20rpx;margin-bottom:20rpx;padding:20rpx 30rpx 20rpx 40rpx;position:relative}.info-list-box .item .info{align-items:center;border-bottom:2rpx dotted #ccc;color:#333;display:-webkit-flex;display:flex;font-size:28rpx;justify-content:flex-start;line-height:1.5;padding:10rpx 0;width:100%}.info-list-box .item .info:first-child,.info-list-box .item .info:last-child{border-bottom:0}.info-list-box .item .info .title{align-self:start;font-weight:700;margin-right:10rpx;position:relative;width:150rpx}.info-list-box .item .info .content{align-items:flex-start;align-self:start;display:-webkit-flex;display:flex;flex:1;justify-content:flex-start}.info-list-box .item .info .center{align-items:center;display:-webkit-flex;display:flex;justify-content:center;width:100%}.info-list-box .item .info .center image{height:400rpx;margin:30rpx;width:400rpx}