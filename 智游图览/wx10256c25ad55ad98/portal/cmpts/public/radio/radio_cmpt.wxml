<radio-group bindchange="bindChange" class="radio-group">
    <view class="item" wx:for="{{options}}" wx:key="key">
        <label class="item-label">
            <radio checked="{{itemSelected==item.value}}" class="item-radio" disabled="{{disabled}}" value="{{item.value}}"></radio>
            <text>{{item.label}}</text>
        </label>
    </view>
</radio-group>
